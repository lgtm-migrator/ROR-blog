<%= form_for @project do |f| %>

    <%= f.text_field :title %>
    <br/>
    <%= f.url_field :link %>
    <br/>
    <%= f.text_area :description %>
    <br/>
    <ul>
    <%= f.fields_for :missions do |f_mission| %>
          <li>
      <%= f_mission.text_field :title %>
      <%= f_mission.text_area :content %>
          </li>
    <% end %>
    <br/>
    </ul>
    <%= f.submit %>
<% end %>